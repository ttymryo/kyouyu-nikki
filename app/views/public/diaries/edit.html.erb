<div class="container">
  <div class="col-12 card w-80 mx-auto mt-4 shadow-sm">
    <div class="card-body">
      <%= form_with model: @diary, url: user_diary_path(current_user.name_id, @diary), method: :patch do |f| %>
        <%= render 'form', f: f %>
        <%= f.submit '編集' %>
      <% end %>
    </div>
  </div>
</div>